  },
  "homepage": "https://github.com/swytch-x/prettier-config#readme",
  "devDependencies": {
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1"
  },
  "prettier": "./index.json",
  "husky": {
